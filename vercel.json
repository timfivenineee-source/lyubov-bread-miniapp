{
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "api/send-feedback.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/send-feedback",
      "dest": "/api/send-feedback.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1"
    }
  ]
}